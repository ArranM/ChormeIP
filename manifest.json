{
    "name": "IPV4",
    "version": "1.0",
    "description": "Copy current IP address to clipboard",
    "host_permissions": [
        "*://*.ipify.org/*"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_title": "Get IPV4 Address",
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}